{
    "name": "silverstripe/installer",
    "type": "silverstripe-recipe",
    "description": "The SilverStripe Framework Installer",
    "require": {
        "php": ">=5.6.0",
        "silverstripe/recipe-plugin": "^1",
        "silverstripe/recipe-cms": "1.1.1@stable",
        "silverstripe-themes/simple": "~3.2.0",
        "wilr/silverstripe-googlesitemaps": "^2.1",
        "silverstripe/html5": "~2.0.0",
        "symbiote/silverstripe-gridfieldextensions": "^3",
        "undefinedoffset/sortablegridfield": "^2",
        "silverstripe/asset-admin": "^1.1",
        "littlegiant/silverstripe-seo-editor": "^2.1",
        "silverstripe/lumberjack": "dev-master",
        "salted-herring/salted-cropper": "dev-feature/ss4-upgrade",
        "gorriecoe/silverstripe-link": "^1.2",
        "gorriecoe/silverstripe-linkfield": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "extra": {
        "branch-alias": {
            "4.x-dev": "4.2.x-dev",
            "dev-master": "5.x-dev"
        },
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/mysite.yml",
            "app/code/Page.php",
            "app/code/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "install-frameworkmissing.html",
            "install.php",
            "web.config"
        ],
        "expose": [
            "themes/default/css",
            "themes/default/img",
            "themes/default/dist"
        ]
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
